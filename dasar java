import.javax.swing.JOptionPane;
public class InputOutput{
  public static void main (String[] args)
  {
    String str1 = "H";
    String str2 = "E";
    String str3 = "L";
    String str4 = "L";
    String str5 = "O";
    String str6 = str1 + str2 + str3 + str4 +str5 ;
    
    JOptionPane.showMessageDialog(null, str6);
   }
}
    
  
